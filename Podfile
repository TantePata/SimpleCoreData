workspace 'ESGISimpleCoreData.xcworkspace'

## Frameworks targets
abstract_target 'Frameworks' do
	use_frameworks!
	target 'ESGISimpleCoreData-iOS' do
		platform :ios, '8.0'
	end
end

## Tests targets
abstract_target 'Tests' do
	use_frameworks!
	target 'ESGISimpleCoreDataTests-iOS' do
		platform :ios, '8.0'
	end
end

## Samples targets
abstract_target 'Samples' do
	use_frameworks!
	target 'ESGISimpleCoreDataSample-iOS' do
		project 'Samples/ESGISimpleCoreDataSample-iOS/ESGISimpleCoreDataSample-iOS'
		platform :ios, '8.0'
	end
end
